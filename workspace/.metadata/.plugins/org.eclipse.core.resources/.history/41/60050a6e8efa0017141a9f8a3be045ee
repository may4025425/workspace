package edu.fju.exam;

public class Employee {
	int amount ;
	int bonus = 5000;
	int bonus2 = 3000;
	
	public Employee(String string, int amount) {
		String a = "Jack";
		String b = "Mary";
		String c = "Danny";
		this.amount = amount;
	}
	
	public void print() {
		
		System.out.println( "a" +"\t"+amount );
		System.out.println( "b" +"\t"+amount );
		System.out.println( "c" +"\t"+amount );
	}
}
