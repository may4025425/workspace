package edu.fju.exam;

public class Employee {
	int amount ;
	int bonus = 5000;
	int bonus2 = 3000;
	
	public Employee(String string, int amount) {
		String s1 = "Jack";
		String s2 = "Mary";
		String s3 = "Danny";
		this.amount = amount;
	}
	
	public void print() {
		int total = amount*bonus;
		System.out.println(amount +"\t"+total );
	}
}
